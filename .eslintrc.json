{
	"extends": [
	  "react-app",
	],
	"env": {
		"node": true,
		"es2022": true,
		"jest": true
	  },
	  "parserOptions": {
		"ecmaVersion": "latest"
	  },
	"rules": {
	  "prefer-const": "error",
	  "semi": ["warn", "always"],
	  "react/jsx-filename-extension": ["error", { "extensions": [".js", ".jsx"] }]
	}
  }
  
  